   "source": [
    "from moviepy.editor import *\n",
    "\n",
    "def convertTime(time):\n",
    "    p1, p2 = time.split(\":\")\n",
    "    p1, p2=  int(p1), int(p2)\n",
    "    return p1*60+p2\n",
    "\n",
    "def trimVideo(data):\n",
    "    video = data[\"video\"]\n",
    "    vid = VideoFileClip(f\"/kaggle/input/crime-dataset-splitted/Crime Train/{video}\")\n",
    "    start, end = convertTime(data[\"start\"]), convertTime(data[\"end\"])\n",
    "    trim = vid.subclip(start, end)\n",
    "    trim.write_videofile(f\"/kaggle/working/Crime Trimmed/{video}\", codec=\"libx264\")\n",
    "    \n",
    "    print(video, start, end)"
   ]
  },
