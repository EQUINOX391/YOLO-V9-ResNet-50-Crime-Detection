     ]
    }
   ],
   "source": [
    "import os\n",
    "import pandas as pd\n",
    "from moviepy.editor import * \n",
    "\n",
    "def convertTime(time):\n",
    "    p1, p2 = time.split(\":\")\n",
    "    p1, p2 = int(p1), int(p2)\n",
    "    return p1*60 + p2\n",
    "\n",
    "def trimVideo(data):\n",
    "    video = data[\"video\"]\n",
    "\n",
    "    try:\n",
    "        vid = VideoFileClip(f\"./Crime/{video}\")\n",
    "        start, end = convertTime(data[\"start\"]), convertTime(data[\"end\"])\n",
    "        \n",
    "        # Memotong video di luar rentang waktu kejadian\n",
    "        clips = []\n",
    "        if start > 0:\n",
    "            clips.append(vid.subclip(0, start))\n",
    "        if end < vid.duration:\n",
    "            clips.append(vid.subclip(end, vid.duration))\n",
    "        \n",
    "        if clips:\n",
    "            final_clip = concatenate_videoclips(clips)\n",
    "            final_clip.write_videofile(f\"./Crime - Normal/{video}\", codec=\"libx264\")\n",
    "            print(f\"Video '{video}' (normal) berhasil disimpan.\")\n",
    "    except Exception as e:\n",
    "            print(f\"Error saat memproses video {video}: {e}\")\n",
    "\n",
    "for info in infos:\n",
    "    trimVideo(info)"
   ]
  },
