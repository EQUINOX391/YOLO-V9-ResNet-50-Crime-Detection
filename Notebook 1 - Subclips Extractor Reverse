   "source": [
    "# Path folder sumber\n",
    "source_folders = [\n",
    "    \"/kaggle/input/anomalydetectiondatasetucf/Anomaly-Videos-Part-4/Anomaly-Videos-Part-4/Shoplifting\",\n",
    "    \"/kaggle/input/anomalydetectiondatasetucf/Anomaly-Videos-Part-4/Anomaly-Videos-Part-4/Stealing\",\n",
    "    \"/kaggle/input/anomalydetectiondatasetucf/Anomaly-Videos-Part_2/Anomaly-Videos-Part-2/Burglary\"\n",
    "]\n",
    "\n",
    "# Menyalin isi folder sumber ke dalam folder 'crime'\n",
    "for source_folder in source_folders:\n",
    "    for item in os.listdir(source_folder):\n",
    "        source_item = os.path.join(source_folder, item)\n",
    "        dest_item = os.path.join(\"Crime\", item)\n",
    "        \n",
    "        # Menyalin file atau folder\n",
    "        if os.path.isfile(source_item):\n",
    "            shutil.copy2(source_item, dest_item)\n",
    "        elif os.path.isdir(source_item):\n",
    "            shutil.copytree(source_item, dest_item)\n",
    "\n",
    "print(\"Folder 'crime' telah dibuat dan diisi dengan data.\")"
   ]
  },
